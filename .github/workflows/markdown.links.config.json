{
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/content/"
    }
	  ],
  "ignorePatterns": [
    {
      "pattern": "http[s]?://(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$"
    },
    {
      "pattern": "http[s]?://www.hackthebox.com/.*"
    },
    {
      "pattern": "http[s]?://www.hackthebox.eu/.*"
    },
    {
      "pattern": "http[s]?://.*.htb"
    }
  ],
  "timeout": "2s",
  "retryOn429": false,
  "retryCount": 1
}
